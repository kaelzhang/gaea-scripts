{
  "name": "gaea-scripts",
  "version": "0.0.2",
  "description": "Command line toolkit for gaea",
  "bin": {
    "gaea-publish": "bin/publish.js",
    "gaea-dev": "bin/dev.js",
    "gaea-start": "bin/start.js",
    "gaea-pack": "bin/pack.js",
    "gaea-docker": "bin/docker.js"
  },
  "scripts": {
    "test": "nyc ava --timeout=10s",
    "test-no-report": "NODE_DEBUG=gaea-scripts nyc ava --timeout=10s --verbose",
    "lint": "eslint .",
    "posttest": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "files": [
    "bin/"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/kaelzhang/gaea-scripts.git"
  },
  "keywords": [
    "gaea-scripts",
    "gaea"
  ],
  "engines": {
    "node": ">= 6"
  },
  "ava": {
    "babel": false
  },
  "author": "kaelzhang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaelzhang/gaea-scripts/issues"
  },
  "devDependencies": {
    "ava": ">= 1.0.0-beta.6",
    "codecov": "^3.0.2",
    "nyc": "^12.0.2",
    "eslint-config-ostai": "^1.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint": "^5.1.0"
  },
  "dependencies": {
    "@ostai/env": "^1.0.0",
    "chalk": "^2.4.1",
    "core-util-is": "^1.0.2",
    "cross-spawn": "^6.0.5",
    "fs-extra": "^7.0.0",
    "glob-gitignore": "^1.0.10",
    "ignore": "^5.0.2",
    "minimist": "^1.2.0",
    "skema": "^9.3.7",
    "tmp": "0.0.33"
  }
}
